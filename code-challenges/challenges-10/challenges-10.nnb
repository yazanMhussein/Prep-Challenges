                function birthdayCakeCandles(candles) {
                                                        let max =0;
                                                        let arr= 0;
                                                        for(let i=0; i< candles.length;i++){
                                                                if(max <  candles[i]){
                                                                max = candles[i]
                                                            }
                                                        }
                                                        for(let i=0; i<candles.length;i++){
                                                                if(max == candles[i]){
                                                                arr++;
                                                            }
                                                        }
                                                                return arr;
                                                        }
            "


